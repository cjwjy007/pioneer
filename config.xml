<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.pioneer" version="1.0.0" xmlns="http://www.w3.org/ns/widgets">
    <name>pioneer</name>
    <description>
    pioneer
  </description>
    <author email="317694683@qq.com" href="http://cordova.io">
    wei
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="./src/assets/images/logo_green.png" />
        <splash src="./src/assets/images/Babel.jpg" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon src="./src/assets/images/logo_green.png" />
        <splash src="./src/assets/images/Babel.jpg" />
    </platform>
    <engine name="ios" spec="^4.5.2" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
